  },
  "scripts": {
    "build:development": "webpack --config ./config/webpack.development.config.js",
    "build:production": "webpack --config ./config/webpack.production.config.js",
    "build:development:watch": "webpack --config ./config/webpack.development.config.js --watch",
    "build:production:watch": "webpack --config ./config/webpack.production.config.js --watch",
    "lint": "yarn run lint:tslint && yarn run lint:eslint",
    "lint:eslint": "eslint -c .eslintrc.json '{config,public,src}/**/*.{js,jsx}'",
    "lint:tslint": "tslint -c tslint.json --project tsconfig.json '{config,public,src}/**/*.{ts,tsx}'",
    "postinstall": "yarn run postinstall:git-lfs && yarn run postinstall:husky",
    "postinstall:git-lfs": "git lfs update --force",
    "postinstall:husky": "husky install",
    "ci": "yarn install --frozen-lockfile"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
